{
  "name": "sveltekit-pocketbase-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "typegen": "pocketbase-typegen --db ./pb_data/data.db --out src/lib/pocketbase-types.ts",
    "hooks:dev": "vite build --watch --config hooks/vite.config.ts",
    "hooks:build": "vite build --config hooks/vite.config.ts"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.2.1",
    "@iconify-json/simple-icons": "^1.2.9",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.7.3",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@unocss/extractor-svelte": "^0.63.6",
    "@unocss/reset": "^0.63.6",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.453.0",
    "mode-watcher": "^0.4.1",
    "pocketbase": "^0.21.5",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.1.3",
    "svelte-check": "^4.0.5",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "unocss": "^0.63.6",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-shadcn": "^0.3.1",
    "vite": "^5.4.10"
  },
  "type": "module",
  "dependencies": {
    "svelte-sonner": "^0.3.28"
  }
}
