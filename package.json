{
  "name": "sveltekit-pocketbase-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "typegen": "pocketbase-typegen --db ./pb_data/data.db --out src/lib/pocketbase-types.ts",
    "hooks:dev": "vite build --watch --config hooks/vite.config.ts",
    "hooks:build": "vite build --config hooks/vite.config.ts"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.1.13",
    "@iconify-json/simple-icons": "^1.1.106",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.16",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@unocss/extractor-svelte": "^0.59.4",
    "@unocss/reset": "^0.59.4",
    "bits-ui": "^0.21.10",
    "clsx": "^2.1.1",
    "lucide-svelte": "^0.368.0",
    "mode-watcher": "^0.3.1",
    "pocketbase": "^0.21.3",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tslib": "^2.6.3",
    "typescript": "^5.0.0",
    "unocss": "^0.59.4",
    "unocss-preset-animations": "^1.0.2",
    "unocss-preset-shadcn": "^0.2.3",
    "vite": "^5.3.1"
  },
  "type": "module",
  "dependencies": {
    "svelte-sonner": "^0.3.24"
  }
}
